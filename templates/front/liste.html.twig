{% extends 'base.html.twig' %}


{% block title  %}Les evenements {% endblock %}

{% block middle %}
<div style="position: absolute; margin-left: 500px; margin-top: 100px">
    <h4>List Events</h4>

    
    <table class="table table-striped">
    <tr>
        
        <td>Titre</td>
        <td>Date</td>
        <td>Description</td>
        <td>image</td>
        <td>lieu</td>
         



    </tr>
    {% for i in tabEvents %}
        <tr>

           
            <td>{{ i.titre }}</td>
            <td>{{i.date|date("m/d/Y") }}</td>
            <td>{{ i.description }}</td>
            <td>{{ i.img }}</td>
            <td>{{ i.lieu }}</td>
            <td><a href="{{ path('app_participer',{'id':i.id}) }}"  class="btn btn-primary" tabindex="-1" role="button" >Participer</a> </td>
           
            <br>
        </tr>

</div>
    {% endfor %}
    </table>{% endblock %}